{
  "title": "Hummingbot Observability",
  "panels": [
    {
      "type": "timeseries",
      "title": "REST p95 latency",
      "targets": [
        {"expr": "histogram_quantile(0.95, rate(hummingbot_rest_request_duration_seconds_bucket[5m]))"}
      ]
    },
    {
      "type": "timeseries",
      "title": "Error streak",
      "targets": [
        {"expr": "max_over_time(hummingbot_error_streak_length[15m])"}
      ]
    },
    {
      "type": "timeseries",
      "title": "Rate-limit tokens remaining",
      "targets": [
        {"expr": "min_over_time(hummingbot_rate_limit_tokens_remaining[5m])"}
      ]
    },
    {
      "type": "timeseries",
      "title": "Funding PnL (realized vs expected)",
      "targets": [
        {"expr": "rate(hummingbot_funding_pnl_realized_usd_total[15m])"},
        {"expr": "avg_over_time(hummingbot_funding_pnl_expected_usd[15m])"}
      ]
    },
    {
      "type": "timeseries",
      "title": "Time to settlement",
      "targets": [
        {"expr": "min_over_time(hummingbot_time_to_settlement_seconds[15m])"}
      ]
    }
  ],
  "templating": {"list": []}
}
